@prefix amr-core: <http://amr.isi.edu/rdf/core-amr#> .
@prefix amr-terms: <http://amr.isi.edu/rdf/amr-terms#> .
@prefix propbank: <http://amr.isi.edu/frames/ld/v1.2.2/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

amr-core:Concept a rdfs:Class ;
    rdfs:label "AMR-Concept" .

amr-core:Role a rdfs:Class ;
    rdfs:label "AMR-Role" .

<http://amr.isi.edu/amr_data/article.sentence1#root01> a amr-core:AMR ;
    amr-core:has-id "article.sentence1" ;
    amr-core:has-sentence "The sheep sleeps on the grass" ;
    amr-core:root <http://amr.isi.edu/amr_data/article.sentence1#s> .

propbank:sleep-01.ARG0 a propbank:FrameRole .

amr-terms:location a amr-core:Role .

amr-core:NamedEntity a amr-core:Concept ;
    rdfs:label "AMR-EntityType",
        "AMR-Term" .

<http://amr.isi.edu/amr_data/article.sentence1#g> a amr-terms:grass .

<http://amr.isi.edu/amr_data/article.sentence1#s> a propbank:sleep-01 ;
    propbank:sleep-01.ARG0 <http://amr.isi.edu/amr_data/article.sentence1#s2> ;
    amr-terms:location <http://amr.isi.edu/amr_data/article.sentence1#g> .

<http://amr.isi.edu/amr_data/article.sentence1#s2> a amr-terms:sheep .

propbank:FrameRole a amr-core:Role ;
    rdfs:label "AMR-PropBank-Role" .

propbank:sleep-01 a amr-core:Frame .

amr-terms:grass a amr-core:Concept .

amr-terms:sheep a amr-core:Concept .

amr-core:Frame a amr-core:Concept ;
    rdfs:label "AMR-PropBank-Frame" .

